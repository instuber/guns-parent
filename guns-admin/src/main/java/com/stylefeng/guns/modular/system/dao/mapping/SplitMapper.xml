<mapper namespace="com.stylefeng.guns.modular.system.dao.SplitMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.Split">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="num" property="num" />
